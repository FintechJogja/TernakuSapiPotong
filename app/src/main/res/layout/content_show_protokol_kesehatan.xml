<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_show_protokol_kesehatan"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="#e2e8e9"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".TernakPerah.Main.TambahData.Kesehatan.ProtocolKesehatan.ShowProtokolKesehatan"
    tools:showIn="@layout/activity_show_protokol_kesehatan">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#fff">
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tambah protokol baru"
            android:id="@+id/button_showprotokolkesehatan_activity_tambah"
            style="?android:attr/borderlessButtonStyle"
            android:drawableRight="@drawable/ic_action_add_orange"
            android:textColor="@color/color_primary_kesehatan"
            android:layout_margin="5dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="5dp"
        android:background="#fff">
        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/prog_showprotokolkesehatan_activity"
            android:indeterminate="true"
            android:layout_gravity="center_horizontal"/>
        <ListView
            android:id="@+id/list_showprotokolkesehatan_activity"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:scrollbars="none" />
    </LinearLayout>

</LinearLayout>
